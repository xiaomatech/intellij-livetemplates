<templateSet group="sass">
  <template name="s-$" value="    $$var1$: $var0$&#10;$END$" description="" toReformat="true" toShortenFQNames="true">
    <variable name="var1" expression="" defaultValue="&quot;variable&quot;" alwaysStopAt="true"/>
    <variable name="var0" expression="" defaultValue="&quot;value&quot;" alwaysStopAt="true"/>
    <context>
      <option name="CSS" value="true"/>
      <option name="HTML" value="true"/>
    </context>
  </template>
  <template name="s-imp" value="    @import '$END$'&#10;" description="" toReformat="true" toShortenFQNames="true">
    <context>
      <option name="CSS" value="true"/>
      <option name="HTML" value="true"/>
    </context>
  </template>
  <template name="s-mix" value="    @mixin $var1$($var2$)&#10;        $END$&#10;" description="" toReformat="true" toShortenFQNames="true">
    <variable name="var1" expression="" defaultValue="&quot;name&quot;" alwaysStopAt="true"/>
    <variable name="var2" expression="" defaultValue="" alwaysStopAt="true"/>
    <context>
      <option name="CSS" value="true"/>
      <option name="HTML" value="true"/>
    </context>
  </template>
  <template name="s-inc" value="    @include $var1$($var2$)&#10;" description="" toReformat="true" toShortenFQNames="true">
    <variable name="var1" expression="" defaultValue="&quot;mixin&quot;" alwaysStopAt="true"/>
    <variable name="var2" expression="" defaultValue="" alwaysStopAt="true"/>
    <context>
      <option name="CSS" value="true"/>
      <option name="HTML" value="true"/>
    </context>
  </template>
  <template name="s-ext" value="    @extend $END$&#10;" description="" toReformat="true" toShortenFQNames="true">
    <context>
      <option name="CSS" value="true"/>
      <option name="HTML" value="true"/>
    </context>
  </template>
  <template name="s-fun" value="    @function $var1$($var2$)&#10;        $END$&#10;" description="" toReformat="true" toShortenFQNames="true">
    <variable name="var1" expression="" defaultValue="&quot;name&quot;" alwaysStopAt="true"/>
    <variable name="var2" expression="" defaultValue="&quot;args&quot;" alwaysStopAt="true"/>
    <context>
      <option name="CSS" value="true"/>
      <option name="HTML" value="true"/>
    </context>
  </template>
  <template name="s-if" value="    @if $var1$&#10;        $END$&#10;" description="" toReformat="true" toShortenFQNames="true">
    <variable name="var1" expression="" defaultValue="&quot;condition&quot;" alwaysStopAt="true"/>
    <context>
      <option name="CSS" value="true"/>
      <option name="HTML" value="true"/>
    </context>
  </template>
  <template name="s-ife" value="    @if $var1$&#10;        $var2$&#10;    @else&#10;        $END$&#10;" description="" toReformat="true" toShortenFQNames="true">
    <variable name="var1" expression="" defaultValue="&quot;condition&quot;" alwaysStopAt="true"/>
    <variable name="var2" expression="" defaultValue="" alwaysStopAt="true"/>
    <context>
      <option name="CSS" value="true"/>
      <option name="HTML" value="true"/>
    </context>
  </template>
  <template name="s-eif" value="    @else if $var1$&#10;        $END$&#10;" description="" toReformat="true" toShortenFQNames="true">
    <variable name="var1" expression="" defaultValue="&quot;condition&quot;" alwaysStopAt="true"/>
    <context>
      <option name="CSS" value="true"/>
      <option name="HTML" value="true"/>
    </context>
  </template>
  <template name="s-for" value="    @for $var1$ from $var2$ through $var3$&#10;        $END$&#10;" description="" toReformat="true" toShortenFQNames="true">
    <variable name="var1" expression="" defaultValue="&quot;$i&quot;" alwaysStopAt="true"/>
    <variable name="var2" expression="" defaultValue="&quot;1&quot;" alwaysStopAt="true"/>
    <variable name="var3" expression="" defaultValue="&quot;3&quot;" alwaysStopAt="true"/>
    <context>
      <option name="CSS" value="true"/>
      <option name="HTML" value="true"/>
    </context>
  </template>
  <template name="s-each" value="    @each $var1$ in $var2$&#10;        $END$&#10;" description="" toReformat="true" toShortenFQNames="true">
    <variable name="var1" expression="" defaultValue="&quot;$item&quot;" alwaysStopAt="true"/>
    <variable name="var2" expression="" defaultValue="&quot;items&quot;" alwaysStopAt="true"/>
    <context>
      <option name="CSS" value="true"/>
      <option name="HTML" value="true"/>
    </context>
  </template>
</templateSet>
